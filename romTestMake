## for GCC
CC=c99
CFLAGS= -g -Wall -pedantic

#files
SOURCE= romtest.c rom.c rom.h
PROG= romTest
	

all: clean $(PROG)

build: $(PROG)

#build romTest
$(PROG): $(SOURCE)
	$(CC) $(CFLAGS) $(SOURCE) -o $(PROG)

clean:
	rm $(PROG)
	echo Clean done
